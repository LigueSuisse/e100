<!DOCTYPE html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>{% block title%}{% endblock %}</title>
    <meta name="description" content="{% block meta_description%}{% endblock %}">

    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="shortcut icon" type="image/x-icon" href="{{ app.request.basepath }}/image/favicon.ico" />

    <link href="{{ app.request.basepath }}/assets/css/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="{{ app.request.basepath }}/assets/css/responsive.css" rel="stylesheet" type="text/css" />

<!--[if lt IE 9]>
<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
<![endif]-->

<script type="text/javascript">

var _gaq = _gaq || [];
_gaq.push(['_setAccount', 'UA-XXXXXX-1']);
_gaq.push(['_trackPageview']);

(function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
})();

</script>
</head>
<body>

<!-- Navbar
    ================================================== -->
    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner">
        <div class="container">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a class="brand" href="{{ path('homepage_i18n') }}">E100</a>
          <div class="nav-collapse collapse">
              {% include '_menu.html.twig' %}
          </div><!--/.nav-collapse -->
        </div>
      </div>
    </div>


    <div class="container">
        {% block content %}{% endblock %}
        <hr>

        <footer>
          <p>&copy; E100 Challenge 2012</p>
        </footer>
    </div>


    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="{{ app.request.basepath }}/assets/js/jquery.js"><\/script>')</script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-transition.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-alert.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-modal.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-dropdown.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-scrollspy.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-tab.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-tooltip.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-popover.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-button.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-collapse.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-carousel.js"></script>
    <script src="{{ app.request.basepath }}/assets/js/bootstrap-typeahead.js"></script>
</body>
</html>
